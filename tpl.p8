version 18
__lua__
function start(e)
	if e.i then
		e.i()
	end
	add(ent,e)
end

function _update()
	tick()
	i=1
	while i<=#ent do
		ent[i].t()
		if ent[i].d then
			del(ent,ent[i])
		else
			i=i+1
		end
	end
end

-- game code

ent={}

player={
	x=0,
	y=0,
	t=function()
	end
}
start(player)

function tick()
	cls(0)
end


