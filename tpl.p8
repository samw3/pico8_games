ent={}

function start(e)
	if e.i then
		e:i()
	end
	add(ent,e)
end

function _init()
	init()
end

function _update()
	tick()
	i=1
	while i<=#ent do
		ent[i]:t()
		if ent[i].d then
			del(ent,ent[i])
		else
			i=i+1
		end
	end
end

